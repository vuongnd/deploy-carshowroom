            <%= image_tag car.image.url(:medium), lazy: true %>
            <%= image_tag car.typeofcar.company.logo.url(), lazy: true %>
          </div>
          <div class="caption">
            <h3><a href="viewcar.html" title=""><%= link_to car.name, car %></a></h3>
            <p><%= car.prices %> $</p>
          </div>
        </div>
      </div>
    <% end %>
